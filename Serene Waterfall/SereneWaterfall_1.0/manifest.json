          "gx_secondary_base": {
            "h": 245,
            "s": 100,
            "l": 59
          }
        },
        "light": {
          "gx_accent": {
            "h": 172,
            "s": 100,
            "l": 75
          },
          "gx_secondary_base": {
            "h": 245,
            "s": 100,
            "l": 75
          }
        }
      },
